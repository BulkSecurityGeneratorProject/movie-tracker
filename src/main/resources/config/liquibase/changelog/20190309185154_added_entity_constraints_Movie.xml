<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.5.xsd">
    <!--
        Added the constraints for entity Movie.
    -->
    <changeSet id="20190309185154-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="rated_id"
                                 baseTableName="movie"
                                 constraintName="fk_movie_rated_id"
                                 referencedColumnNames="id"
                                 referencedTableName="rated"/>

        <addForeignKeyConstraint baseColumnNames="production_id"
                                 baseTableName="movie"
                                 constraintName="fk_movie_production_id"
                                 referencedColumnNames="id"
                                 referencedTableName="production"/>

    </changeSet>
</databaseChangeLog>
